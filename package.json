{
  "name": "bibliomap-viewer",
  "version": "3.0.3",
  "description": "BiblioMap (viewer) shows ezpaarse geolocalized EC's on a map",
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "insertversion": "sed -i \"s#\\(image: ezpaarseproject/bibliomap-viewer:\\)\\([\\.a-z0-9]\\+\\)#\\1${npm_package_version}#g\" docker-compose.yml && sed -i \"s#\\(Bibliomap \\)\\([\\.0-9]\\+\\)#\\1${npm_package_version}#g\" index*.html",
    "version": "npm run insertversion && git add docker-compose.yml index*.html ",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ezpaarse-project/bibliomap-viewer.git"
  },
  "author": "St√©phane Gully <stephane.gully@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ezpaarse-project/bibliomap-viewer/issues"
  },
  "homepage": "https://github.com/ezpaarse-project/bibliomap-viewer",
  "dependencies": {
    "JSONStream": "^0.9.0",
    "debug": "^2.6.0",
    "event-stream": "^3.1.7",
    "express": "^4.9.5",
    "nconf": "^0.6.9",
    "nodemon": "^1.9.1",
    "request": "^2.39.0",
    "socket.io": "^1.1.0"
  },
  "devDependencies": {
    "mocha": "^2.4.5"
  }
}
